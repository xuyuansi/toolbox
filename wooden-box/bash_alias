#.bash_aliases

#-----------------
alias lal='ls -al'
alias la='ls -a'
alias ll='ls -l'
alias lf='ls -F'
alias laf='ls -aF'
#-----------------

#---------------------------------------------------------------------
alias de='cd ~/Desktop'
alias vipf='vi ~/.profile'
alias srpf='source ~/.profile'
#---------------------------------------------------------------------

#---------------------------------------------------------------------
alias env='/usr/bin/env |sort' 
alias c='clear'
#---------------------------------------------------------------------
